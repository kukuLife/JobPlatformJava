<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
        <!--The package name of your interface is com.abc.dao,The interface name is NameMapper.javaï¼ŒSo your mapper.xml Of namespace Should be com.abc.dao.NameMapper-->
<mapper namespace="com.jobplatform.user.mapper.UserMapper" >

     <resultMap id="UserResultMap" type="com.jobplatform.user.entity.User">
        <result column="UI_ID" jdbcType="VARCHAR" property="UI_ID" />
        <result column="UI_PWD" jdbcType="VARCHAR" property="UI_PWD" />
        <result column="USR_KEY_WORD" jdbcType="VARCHAR" property="USR_KEY_WORD" />
        <result column="UI_CreateDate" jdbcType="VARCHAR" property="UI_CreateDate" />
    </resultMap>

    <select id="select" parameterType="String" resultType="com.jobplatform.user.entity.User">
        SELECT * FROM app_UserInfo WHERE UI_ID = #{userId}
    </select>
    
    <insert id="insert" parameterType="String">
        INSERT INTO app_UserInfo(UI_ID, UI_PWD, USR_KEY_WORD, UI_CreateDate) VALUES (#{userId}, #{passWord}, '', SYSDATE())
    </insert>

    

</mapper>