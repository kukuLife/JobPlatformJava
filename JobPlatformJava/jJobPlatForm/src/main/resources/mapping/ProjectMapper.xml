<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
        <!--The package name of your interface is com.abc.dao,The interface name is NameMapper.javaï¼ŒSo your mapper.xml Of namespace Should be com.abc.dao.NameMapper-->
<mapper namespace="com.jobplatform.project.mapper.ProjectMapper" >

     <resultMap id="ProjectResultMap" type="com.jobplatform.project.entity.Project">
        <result column="PJ_ID" jdbcType="VARCHAR" property="PJ_ID" />
        <result column="PJ_NAME" jdbcType="VARCHAR" property="PJ_NAME" />
        <result column="PJ_INFO" jdbcType="VARCHAR" property="PJ_INFO" />
        <result column="PJ_KEY_WORLD" jdbcType="VARCHAR" property="PJ_KEY_WORLD" />
        <result column="TARGET_USER" jdbcType="VARCHAR" property="TARGET_USER" />
        <result column="START_DATE" jdbcType="VARCHAR" property="START_DATE" />
        <result column="END_DATE" jdbcType="VARCHAR" property="END_DATE" />
    </resultMap>

    <select id="select" parameterType="String" resultType="com.jobplatform.project.entity.Project">
        SELECT * FROM app_Project_Info WHERE Target_User=#{userId}
         <if test="projectKeyWord != null and projectKeyWord != ''">
         	and PJ_KEY_WORLD = #{projectKeyWord}
         </if>
    </select>

</mapper>